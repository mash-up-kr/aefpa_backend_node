{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/kingbob-server/src",
  "monorepo": true,
  "root": "apps/kingbob-server",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/kingbob-server/tsconfig.app.json"
  },
  "projects": {
    "kingbob-server": {
      "type": "application",
      "root": "apps/kingbob-server",
      "entryFile": "main",
      "sourceRoot": "apps/kingbob-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/kingbob-server/tsconfig.app.json"
      }
    },
    "utils": {
      "type": "library",
      "root": "libs/utils",
      "entryFile": "index",
      "sourceRoot": "libs/utils/src",
      "compilerOptions": {
        "tsConfigPath": "libs/utils/tsconfig.lib.json"
      }
    }
  }
}
