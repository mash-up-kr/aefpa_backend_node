{
  "name": "kki-log-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "cacheDirectories": [
    ".yarn/cache"
  ],
  "engines": {
    "node": "16.13.1",
    "yarn": "3.2.1"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rm -rf dist; yarn db:generate && nest build && cp -r ./src/api ./dist/src",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "yarn build && nest start",
    "start:dev": "yarn build && nest start --watch",
    "start:quick": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:generate": "yarn pnpify prisma generate",
    "db:migrate": "yarn pnpify prisma migrate dev"
  },
  "dependencies": {
    "@jest/types": "^28.1.1",
    "@nestjs-modules/mailer": "^1.7.1",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.2.1",
    "@prisma/client": "3.15.2",
    "@yarnpkg/pnpify": "^4.0.0-rc.10",
    "aws-sdk": "^2.1171.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "express": "^4.18.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "nodemailer": "^6.7.7",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.4.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/lodash": "^4",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6",
    "@types/passport": "^0",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1",
    "@types/source-map-support": "^0.5.4",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@yarnpkg/sdks": "^3.0.0-rc.10",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "prettier": "^2.3.2",
    "prisma": "3.15.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "packageManager": "yarn@3.2.1"
}
